      <umbNewsItem id="1075" parentID="1074" level="3" creatorID="0" sortOrder="0" createDate="2013-12-17T09:39:06" updateDate="2013-12-18T23:49:30" nodeName="Partner i Form Works, Dan verbaum som jury" urlName="partner-i-form-works-dan-verbaum-som-jury" path="-1,1069,1074,1075" isDoc="" nodeType="1058" creatorName="ThomasBoeriis" writerName="ThomasBoeriis" writerID="0" template="1053" nodeTypeAlias="umbNewsItem">
        <umbracoNaviHide>1</umbracoNaviHide>
