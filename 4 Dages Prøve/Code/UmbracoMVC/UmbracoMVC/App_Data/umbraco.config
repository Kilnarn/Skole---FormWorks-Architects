      <GalleryItem id="1189" parentID="1218" level="3" creatorID="0" sortOrder="2" createDate="2013-12-17T12:56:59" updateDate="2013-12-20T14:12:28" nodeName="Curve Projektet" urlName="curve-projektet" path="-1,1069,1218,1189" isDoc="" nodeType="1089" creatorName="ThomasBoeriis" writerName="ThomasBoeriis" writerID="0" template="1088" nodeTypeAlias="GalleryItem">
